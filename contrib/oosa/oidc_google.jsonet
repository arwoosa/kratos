local claims = std.extVar('claims');

if std.length(claims.sub) == 0 then
  error 'claim sub not set'
else
  {
    identity: {
      traits: {
        all: claims,
        username: claims.sub,
        email: claims.email,
        name: claims.name,
        picture: claims.picture,
        language: "zh-TW"
      },
    },
  }